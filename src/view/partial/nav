                        async function tryLogOut () {
                            // console.log(req.session.user)
                            try {
                                const response = await fetch(`http://127.0.0.1:1800/session`, {
                                    method: "DELETE"
                                });
                                const jsonData = await response.json();
                                console.log("data received:", jsonData);
                                setCookie("sid","",0);
                                window.location = "";
